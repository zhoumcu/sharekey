        <meta-data
            android:name="UMENG_APPKEY"
            android:value="58f5f3919f06fd2cb8001b50">
        </meta-data>

        <meta-data
            android:name="UMENG_MESSAGE_SECRET"
            android:value="b58cbd1ce3f7c363bbd5d202b8103826">
        </meta-data>
        <meta-data
            android:name="UMENG_CHANNEL"
            android:value="umeng" >
        </meta-data>

